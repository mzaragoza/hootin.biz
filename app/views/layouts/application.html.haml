!!!
%html{:lang => "en"}
  %head
    %meta{:charset => "utf-8"}/
    %meta{:content => "IE=edge", "http-equiv" => "X-UA-Compatible"}/
    %meta{:content => "width=device-width, initial-scale=1", :name => "viewport"}/
    %meta{:content => "", :name => "description"}/
    %meta{:content => "", :name => "keywords"}/
    %meta{:content => "Hootin", :name => "author"}/
    %title
      - unless yield(:title).to_s.empty?
        = yield (:title)
        |
      Hootin
      |
      (Helping Others Out There In Need)
    %link{:href => "/assets/assets/images/ico/favicon.png", :rel => "shortcut icon"}/
    %link{:href => "/assets/assets/images/ico/apple-touch-icon-144-precomposed.png", :rel => "apple-touch-icon-precomposed", :sizes => "144x144"}/
    %link{:href => "/assets/assets/images/ico/apple-touch-icon-114-precomposed.png", :rel => "apple-touch-icon-precomposed", :sizes => "114x114"}/
    %link{:href => "/assets/assets/images/ico/apple-touch-icon-72-precomposed.png", :rel => "apple-touch-icon-precomposed", :sizes => "72x72"}/
    %link{:href => "/assets/assets/images/ico/apple-touch-icon-57-precomposed.png", :rel => "apple-touch-icon-precomposed"}/
    %link{:href => "http://fonts.googleapis.com/css?family=Open+Sans:400italic,400,300,600,700", :rel => "stylesheet", :type => "text/css"}/
    %link{:href => "/assets/assets/fonts/fontAwesome/css/font-awesome.min.css", :rel => "stylesheet"}/
    %link{:href => "/assets/assets/fonts/flaticon/flaticon.css", :rel => "stylesheet"}/
    %link{:href => "/assets/assets/magnific/magnific-popup.css", :rel => "stylesheet"}/
    %link{:href => "/assets/assets/owl.carousel/assets/owl.carousel.css", :rel => "stylesheet"}/
    %link{:href => "/assets/assets/owl.carousel/assets/owl.theme.min.css", :rel => "stylesheet"}/
    %link{:href => "/assets/assets/bootstrap/css/bootstrap.min.css", :rel => "stylesheet"}/
    %link{:href => "/assets/assets/css/style.css", :rel => "stylesheet"}/
    %link{:href => "/assets/assets/css/responsive.css", :rel => "stylesheet"}/
    = stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true
    = javascript_include_tag 'application', 'data-turbolinks-track' => true
    = csrf_meta_tags
    /[if lt IE 9]
      <script src="https://oss.maxcdn.com/html5shiv/3.7.2/html5shiv.min.js"></script>
      <script src="https://oss.maxcdn.com/respond/1.4.2/respond.min.js"></script>
  %body{"data-spy" => "scroll", "data-target" => ".navbar-fixed-top"}
    %nav.navbar.navbar-default.navbar-fixed-top
      .container
        .navbar-header
          %button.navbar-toggle.collapsed{"data-target" => "#bs-example-navbar-collapse-1", "data-toggle" => "collapse", :type => "button"}
            %span.sr-only Toggle navigation
            %span.icon-bar
            %span.icon-bar
            %span.icon-bar
          %a.navbar-brand{:href => "index.html"}
            %img{:alt => "", :src => "/assets/assets/images/logo.png"}/
        #bs-example-navbar-collapse-1.collapse.navbar-collapse
          %ul.nav.navbar-nav.navbar-right
            %li.active
              %a.page-scroll{:href => "#home"} Home
            %li
              %a.page-scroll{:href => "#about"} About
            %li
              %a.page-scroll{:href => "#mission"} Mission
            %li
              %a.page-scroll{:href => "#story"} Stories
            %li
              %a.page-scroll{:href => "#ability"} Abilities
            %li
              %a.page-scroll{:href => "#reformation"} Reformation
            %li
              %a.page-scroll{:href => "#event"} Event
            %li
              %a.page-scroll{:href => "#press"} Press
            %li
              %a.page-scroll{:href => "#contact"} Contact
    = yield
    %footer.footer-section.text-center
      .container
        %h3 I'm social, You can follow me
        .social-links
          %ul.list-inline
            %li
              %a{:href => "#"}
                %i.fa.fa-facebook
            %li
              %a{:href => "#"}
                %i.fa.fa-twitter
            %li
              %a{:href => "#"}
                %i.fa.fa-google-plus
            %li
              %a{:href => "#"}
                %i.fa.fa-linkedin
            %li
              %a{:href => "#"}
                %i.fa.fa-tumblr
        %span.copyright Hootin - All rights reserved Â©2015
    #preloader
      #status
        .status-mes
    %script{:src => "/assets/assets/js/jquery-2.1.3.min.js"}
    %script{:src => "/assets/assets/bootstrap/js/bootstrap.min.js"}
    %script{:src => "/assets/assets/js/jquery.easing.min.js"}
    %script{:src => "/assets/assets/js/smoothscroll.min.js"}
    %script{:src => "/assets/assets/js/jquery.inview.min.js"}
    %script{:src => "/assets/assets/js/jquery.stellar.min.js"}
    %script{:src => "/assets/assets/js/jquery.countTo.min.js"}
    %script{:src => "/assets/assets/js/coundown-timer.min.js"}
    %script{:src => "/assets/assets/js/jquery.shuffle.min.js"}
    %script{:src => "/assets/assets/js/bootstrap-tabcollapse.min.js"}
    %script{:src => "/assets/assets/magnific/jquery.magnific-popup.min.js"}
    %script{:src => "/assets/assets/js/ajaxchimp.min.js"}
    %script{:src => "/assets/assets/js/twitterFetcher.min.js"}
    %script{:src => "/assets/assets/owl.carousel/owl.carousel.min.js"}
    %script{:src => "https://maps.googleapis.com/maps/api/js"}
    %script{:src => "/assets/assets/js/scripts.js"}
